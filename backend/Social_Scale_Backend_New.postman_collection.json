            "url": {
              "raw": "{{baseUrl}}/api/v1/api-integrations/services?network=Instagram",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "api-integrations", "services"],
              "query": [
                {
                  "key": "network",
                  "value": "Instagram",
                  "description": "Optional: Filter by network (Instagram, TikTok, etc.)"
                },
                {
                  "key": "companyId",
                  "value": "{{companyId}}",
                  "description": "Optional: For logging purposes"
                }
              ]
            }
          }
        },
        {
          "name": "Create Order",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    if (response.apiOrderId) {",
                  "        pm.environment.set(\"fampageOrderId\", response.apiOrderId);",
                  "    }",
                  "    if (response.orderId) {",
                  "        pm.environment.set(\"orderId\", response.orderId);",
                  "    }",
                  "    if (response.invoice && response.invoice._id) {",
                  "        pm.environment.set(\"invoiceId\", response.invoice._id);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"companyId\": \"{{companyId}}\",\n  \"service\": 1,\n  \"link\": \"instagram.com/instagram\",\n  \"quantity\": 1000,\n  \"invoiceMultiplier\": 8\n}"
            },
